{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Steeltoe": "Warning",
      "Sample": "Information"
    }
  },
  "Management": {
    "Endpoints": {
      "Actuator": {
        "Exposure": {
          "Include": [ "*" ]
        }
      },
      "Prometheus": {
        "Id": "promfoo"
      },
      "Metrics": {
        "CacheDurationMilliseconds": 5000
      }
    },
    "Metrics": {
      "Observer": {
        "EventCounterEvents": true
      },
      "export": {
        "wavefront": {
          "uri": "proxy://dummy.com"
        }
      }

    }
  }
}
